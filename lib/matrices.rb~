class Matriz
    
    def initialize(m,n)
      @fil,@col = m, n
      
      i=0      
      while i < @fil
	 @valor[i]=Array.new(@col,0)
	 i=i+1
      end
      
    end
    
  
    def to_s
      i=0
      j=0
	  while i<0
	    while j<0
	      cadena+= "#{valor[i][j]} "
	      j=j+1
	    end
	    
	  cadena+="\n"
	  i=i+1
	end
	
     cadena
     
    end
    
    
    
    